{
  "name": "@sylphx/code-core",
  "version": "0.1.0",
  "description": "Sylphx Code SDK - Complete headless SDK with all business logic",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm --sourcemap",
    "dev": "bun --watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.40",
    "@ai-sdk/google": "^2.0.26",
    "@ai-sdk/openai": "^2.0.59",
    "@ai-sdk/openai-compatible": "^1.0.25",
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "@libsql/client": "^0.15.15",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "ai": "^5.0.86",
    "ai-sdk-provider-claude-code": "^2.1.0",
    "drizzle-orm": "^0.44.7",
    "which": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/node": "^24.9.2",
    "@types/which": "^3.0.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "keywords": [
    "ai",
    "sdk",
    "streaming",
    "session",
    "anthropic",
    "openai",
    "google"
  ],
  "author": "sylphxltd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphxltd/flow.git",
    "directory": "packages/code-core"
  }
}
