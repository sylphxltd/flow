{
  "name": "@sylphx/code-client",
  "version": "0.1.0",
  "description": "Sylphx Code Client - Shared React hooks and components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm --sourcemap",
    "dev": "bun --watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sylphx/code-core": "workspace:*",
    "@sylphx/code-server": "workspace:*",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@tanstack/react-query": "^5.70.0",
    "react": "^19.2.0",
    "zustand": "^5.0.8",
    "immer": "^10.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/node": "^24.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "keywords": [
    "react",
    "hooks",
    "components",
    "trpc",
    "client"
  ],
  "author": "sylphxltd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphxltd/flow.git",
    "directory": "packages/code-client"
  }
}
