    "gradient-string": "^3.0.0",
    "gray-matter": "^4.0.3",
    "ignore": "^7.0.5",
    "immer": "^10.2.0",
    "ink": "^6.4.0",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^12.10.0",
