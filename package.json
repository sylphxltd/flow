{
  "name": "@sylphx/flow",
  "version": "0.2.2",
  "description": "Sylphx Flow - Type-safe development flow for modern web development",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sylphx-flow": "dist/index.js"
  },
  "files": [
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphxltd/flow.git"
  },
  "keywords": [
    "development",
    "flow",
    "guidelines",
    "typescript",
    "nextjs",
    "sveltekit",
    "react",
    "flutter",
    "type-safe",
    "mcp",
    "ai-agents",
    "github-installation"
  ],
  "author": "sylphxltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylphxltd/flow/issues"
  },
  "homepage": "https://github.com/sylphxltd/flow#readme",
  "scripts": {
    "build": "bun run build.ts",
    "prepare": "bun run build.ts",
    "dev": "bun src/index.ts",
    "start": "bun dist/index.js",
    "start:node": "node dist/index.js",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules/.cache",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:chat": "bun src/test-chat.ts",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.3.1",
  "dependencies": {
