{
  "name": "@sylphxltd/flow",
  "version": "1.0.0",
  "description": "Sylphx Flow - Type-safe development flow for modern web development",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sylphx-flow": "dist/index.js"
  },
  "files": ["dist", "docs", "agents", "README.md", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphxltd/flow.git"
  },
  "keywords": [
    "development",
    "flow",
    "guidelines",
    "typescript",
    "nextjs",
    "sveltekit",
    "react",
    "flutter",
    "type-safe",
    "mcp",
    "ai-agents",
    "github-installation"
  ],
  "author": "sylphxltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylphxltd/flow/issues"
  },
  "homepage": "https://github.com/sylphxltd/flow#readme",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --minify --splitting --external react-devtools-core",
    "dev": "tsx index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun build",
    "prepare": "bun run build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.3.0",
  "dependencies": {
    "@effect/cli": "^0.71.0",
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.4",
    "@effect/printer": "^0.46.0",
    "@effect/printer-ansi": "^0.46.0",
    "@effect/schema": "^0.75.5",
    "@effect/sql": "^0.46.0",
    "@libsql/client": "^0.15.15",
    "@modelcontextprotocol/sdk": "^1.18.1",
    "commander": "^12.0.0",
    "effect": "^3.18.4",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.5.2",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
